platform :ios, '10.0'

target 'YelpApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'AFNetworking', '~> 2.5'
  pod 'BDBOAuth1Manager'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKMessengerShareKit';
  pod 'Firebase/Core'
  pod 'Firebase/Database'
end
